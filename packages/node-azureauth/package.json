{
  "name": "azureauth",
  "version": "0.4.6",
  "description": "node-azure auth wrapps the AzureAuth CLI wrapper for performing AAD Authentication",
  "bin": {
    "azureauth": "./cli.js"
  },
  "repository": {
    "url": "https://github.com/microsoft/ado-npm-auth"
  },
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/**/*.ts && prettier --write ./cli.js",
    "start": "tsc --watch",
    "test": "vitest run src",
    "postinstall": "node ./scripts/install.js"
  },
  "keywords": [
    "node",
    "azureauth",
    "aad",
    "azure active directory",
    "authentication"
  ],
  "author": "Jonathan Creamer",
  "license": "MIT",
  "dependencies": {
    "decompress": "^4.2.1",
    "execa": "^7.2.0",
    "node-downloader-helper": "^2.1.9"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0",
    "vite": "^5.2.8"
  }
}
